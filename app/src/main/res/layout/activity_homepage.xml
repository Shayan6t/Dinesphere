<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".homepage">
    <ImageButton
        android:id="@+id/address_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="15dp"
        android:background="@drawable/map_pin_w"/>
    <TextView
        android:id="@+id/curr_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time Square Way, Brooklyn Street"
        android:fontFamily="@font/montserrat_med"
        android:textColor="@color/white"
        android:textSize="14sp"
        android:layout_marginLeft="15dp"
        android:layout_toRightOf="@id/address_icon"
        android:layout_alignTop="@id/address_icon"/>
    <ImageButton
        android:id="@+id/notification_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/address_icon"
        android:layout_marginRight="15dp"
        android:background="@drawable/notification_w"/>
    <EditText
        android:id="@+id/search_bar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:fontFamily="@font/montserrat_med"
        android:layout_below="@id/address_icon"
        android:background="@drawable/edittext_box"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="40dp"/>
    <ImageButton
        android:id="@+id/search_mag_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/search_icon"
        android:layout_alignTop="@id/search_bar"
        android:layout_alignStart="@id/search_bar"
        android:layout_margin="13dp"/>
    <TextView
        android:id="@+id/street_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search for food source"
        android:fontFamily="@font/montserrat_med"
        android:textColor="@color/white"
        android:textSize="13sp"
        android:layout_alignTop="@id/search_bar"
        android:layout_toRightOf="@id/search_mag_icon"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="15dp"/>
    <TextView
        android:id="@+id/navigate_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:gravity="center"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:layout_below="@id/search_bar"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginTop="25dp"
        android:text="@string/navigate_text"
        android:fontFamily="@font/montserrat_med" />
    <TextView
        android:id="@+id/outlets_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Outlets around you"
        android:fontFamily="@font/montserrat_med"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:layout_below="@id/navigate_text"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="20dp"/>
    <TextView
